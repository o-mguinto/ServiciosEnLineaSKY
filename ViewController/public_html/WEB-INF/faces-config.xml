<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <locale-config>
      <default-locale>es</default-locale>
    </locale-config>
  </application>
  <validator>
    <validator-id>validadorEstructuraContrasenia</validator-id>
    <validator-class>mx.com.sky.sel.utils.ValidadorEstructuraContrasenia</validator-class>
  </validator>
  <managed-bean>
    <managed-bean-name>AgentUtil</managed-bean-name>
    <managed-bean-class>mx.com.sky.sel.utils.AgentUtil</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <converter>
    <converter-id>DayConverter</converter-id>
    <converter-class>mx.com.sky.sel.converter.DayConverter</converter-class>
  </converter>
  <converter>
    <converter-id>HorarioConverter</converter-id>
    <converter-class>mx.com.sky.sel.converter.HorarioConverter</converter-class>
  </converter>
</faces-config>