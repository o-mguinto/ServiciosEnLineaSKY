        <af:resource type="javascript">
          $(document).ready(function () {
            $('.menuPrincipal').click(handleClickMenu);
            function handleClickMenu(e) {
                var dropdownContent = this.nextElementSibling;
                var firstChild = this.firstChild;
                var hijos = firstChild.children;
                if (dropdownContent.style.display === "block") {
                //se le asigna la clase de abrir flecha al segundo hijo
                    hijos[1].className = "flecha-abrir skyTxt";
                    dropdownContent.style.display = "none";
                } else {
                //se le asigna la clase de cerrar flecha al segundo hijo
                    hijos[1].className = "flecha-cerrar skyTxt";
                    dropdownContent.style.display = "block";
                }
            };

              $('#show-menu').click(function () {
                  $('#blue-menu').addClass('sm-black-info');
                  $('#blue-menu').slideDown('slow');
                  $('#show-menu').hide();
                  $('#hide-menu').show();
              });

              $('#hide-menu').click(function () {
                  $('#blue-menu').removeClass('sm-black-info');
                  $('#blue-menu').slideUp('slow');
                  $('#hide-menu').hide();
                  $('#show-menu').show();
              });

              var elmntRight = document.getElementById("div-panel-right-frame");
              $('#blue-menu').css("min-height", (elmntRight.offsetHeight+30) + "px");
          });
        </af:resource>

        <af:resource type="css">
            /*fin af:tree*/
            .menuPrincipal{
                padding: 5% 2% 5% 5%;
                min-height: 40px;
                
            }
            .menuPrincipal:hover{
                background-color: rgba(255, 255, 255, 0.5);
                cursor: pointer;
            }
            .flecha-abrir:after {
                            content: '\2228';
                            font-family: 'Sky Text';
                            font-style: normal;
                            font-weight: 700;
                            font-size: 18px;
                            line-height: 21px;
                            /* identical to box height */
    
    
    /* Text/Dark */
    
    color: #373737;
                        }
            .flecha-cerrar:after {
                            content: '\2227';
                            font-family: 'Sky Text';
                            font-style: normal;
                            font-weight: 700;
                            font-size: 18px;
                            line-height: 21px;
                            /* identical to box height */
                            
                            
                            /* Text/Dark */
                            
                            color: #373737;
                        }
            .titulo-paquetes{
                color: white;
                text-transform: uppercase;
                margin-left: 4px;
                font-size: 0.9em;
            }
            .submenu{
                display: black;
            }
            /*para quitar el color del borde del menu seleccionado*/
            .xkm.p_AFSelected{
                border-left: 3px solid transparent;
            }
        </af:resource>

