        <af:popup id="popUpNuevoUsuario">
            <af:panelWindow id="popUpCrearCuenta" closeIconVisible="false"
                            title="#{selSKYBundle['admoncuentas.popUpCrearCuenta.title']}" modal="true">
                <af:panelFormLayout id="pfl1" maxColumns="2" rows="7" fieldWidth="150" labelWidth="210">
                    
                        <af:inputText value="#{backingBeanScope.BBAdministracionUsuario.usuario.userName}"
                                      label="#{selSKYBundle['admoncuentas.it23.label']}" styleClass="text-internal"
                                      required="true" maximumLength="20" id="it23" rendered="false">
                            <af:validateRegExp messageDetailNoMatch="Solo se permiten carateres alfanuméricos"
                                               pattern="\\w+"/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it25.label']}" styleClass="text-internal"
                                      id="it25" value="#{backingBeanScope.BBAdministracionUsuario.usuario.email}"
                                      required="true" maximumLength="40">
                        <af:validateRegExp pattern="[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-\\+]+)*@[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{1,4})(\\s)*"
                                               messageDetailNoMatch="Ingrese una dirección válida"/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it22.label']}" styleClass="text-internal"
                                      id="it22" value="#{backingBeanScope.BBAdministracionUsuario.usuario.nombre}"
                                      required="true" maximumLength="35">
                            <af:validateRegExp messageDetailNoMatch="No se permiten caractéres especiales"
                                               pattern="[A-Za-zñÑ ]+"/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it32.label']}" styleClass="text-internal"
                                      id="it32"
                                      value="#{backingBeanScope.BBAdministracionUsuario.usuario.apellidoPaterno}"
                                      required="true" maximumLength="35">
                            <af:validateRegExp messageDetailNoMatch="No se permiten caractéres especiales"
                                               pattern="[A-Za-zñÑ ]+"/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it24.label']}" styleClass="text-internal"
                                      id="it24"
                                      value="#{backingBeanScope.BBAdministracionUsuario.usuario.apellidoMaterno}"
                                      required="true" maximumLength="35">
                            <af:validateRegExp messageDetailNoMatch="No se permiten caractéres especiales"
                                               pattern="[A-Za-zñÑ ]+"/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it34.label']}" styleClass="text-internal"
                                      id="it34" value="#{backingBeanScope.BBAdministracionUsuario.contraseniaNueva}"
                                      required="true" secret="true">
                            <af:validateLength id="validateLength1" minimum="6" maximum="15"
                                               hintNotInRange="Ingresa de 6 a 15 letras y/o números, es sensible a MAYÚSCULAS y minúsculas."/>
                        </af:inputText>
                         
                        <af:outputFormatted value="#{selSKYBundle['admoncuentas.of1.value']}" id="of1" styleClass="cursiv"
                                            inlineStyle="text-align:justify;"/>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it35.label']}" styleClass="text-internal"
                                      id="it35" value="#{backingBeanScope.BBAdministracionUsuario.contraseniaNuevaC}"
                                      required="true" secret="true">
                            <af:validateLength id="passwdVal" minimum="6" maximum="15"
                                               hintNotInRange="Ingresa de 6 a 15 letras y/o números, es sensible a MAYÚSCULAS y minúsculas."/>
                        </af:inputText>
                         
                        <af:inputText label="#{selSKYBundle['admoncuentas.it26.label']}" styleClass="text-internal"
                                      id="it26" maximumLength="15"
                                      value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoMovil}"
                                      required="true">
                            <af:validateLength id="validateLength2" minimum="8" maximum="10"
                                               hintNotInRange="de 8 a 10 números"/>
                            <af:validateRegExp id="validateRegExp2" pattern="[0-9]{0,10}"
                                               messageDetailNoMatch="Debe ingresar solo números"/>
                        </af:inputText>
                         
                        <af:panelLabelAndMessage label="#{selSKYBundle['admoncuentas.plam1.label']}" id="plam1" styleClass="text-internal">
                            <af:panelFormLayout id="pfl2">
                                <af:inputText label="Lada" id="it27" styleClass="text-internal"
                                              value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoLada}"
                                              maximumLength="5">
                                    <af:validateRegExp id="ladaVal" pattern="[0-9]{0,4}"
                                                       messageDetailNoMatch="Hasta 4 números"/>
                                </af:inputText>
                                <af:inputText label="Teléfono principal" id="it28" styleClass="text-internal"
                                              value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoCasa}"
                                              maximumLength="15">
                                    <af:validateLength id="validateLength3" minimum="8" maximum="10"
                                                       hintNotInRange="de 8 a 10 números"/>
                                    <af:validateRegExp id="validateRegExp3" pattern="[0-9]{0,10}"
                                                       messageDetailNoMatch="Debe ingresar solo números"/>
                                </af:inputText>
                            </af:panelFormLayout>
                        </af:panelLabelAndMessage>
                         
                        <af:panelLabelAndMessage label="#{selSKYBundle['admoncuentas.plam2.label']}" styleClass="text-internal" id="plam2">
                            <af:panelFormLayout id="pfl3">
                                <af:inputText label="Lada" id="it29" styleClass="text-internal"
                                              value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoTrabajoLada}"
                                              maximumLength="3">
                                    <af:validateRegExp id="validateRegExp1" pattern="[0-9]{0,4}"
                                                       messageDetailNoMatch="Hasta 4 números"/>
                                </af:inputText>
                                <af:inputText label="Teléfono" id="it30" styleClass="text-internal"
                                              value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoTrabajo}"
                                              maximumLength="15">
                                    <af:validateLength id="validateLength4" minimum="8" maximum="10"
                                                       hintNotInRange="de 8 a 10 números"/>
                                    <af:validateRegExp id="validateRegExp4" pattern="[0-9]{0,10}"
                                                       messageDetailNoMatch="Debe ingresar solo números"/>
                                </af:inputText>
                                <af:inputText label="Ext" id="it31" styleClass="text-internal"
                                              value="#{backingBeanScope.BBAdministracionUsuario.usuario.telefonoTrabajoExt}"
                                              maximumLength="10">
                                    <af:validateRegExp id="validateRegExp5" pattern="[0-9]{0,8}"
                                                       messageDetailNoMatch="Hasta 8 números"/>
                                </af:inputText>
                            </af:panelFormLayout>
                        </af:panelLabelAndMessage>
                    
                    <f:facet name="footer">
                        <af:group id="g1">
                            <af:panelGroupLayout id="pnlg1">
                                <span class="text-internal xf" style="margin-left:30px;">* </span>
                                <af:outputText value="#{selSKYBundle['general.obligatorio']}" id="ot11"
                                               styleClass="cursiv"/>
                            </af:panelGroupLayout>
                        </af:group>
                    </f:facet>
                </af:panelFormLayout>
                <af:panelGroupLayout id="pgl4">
