                                                                  required="#{bindings.UsuarioRegistro.hints.telefonoTrabajo.mandatory}"
                                                                  columns="#{bindings.UsuarioRegistro.hints.telefonoTrabajo.displayWidth}"
                                                                  maximumLength="#{bindings.UsuarioRegistro.hints.telefonoTrabajo.precision}"
                                                                  shortDesc="#{bindings.UsuarioRegistro.hints.telefonoTrabajo.tooltip}"
                                                                  id="it6" readOnly="true">
                                                        <f:validator binding="#{row.bindings.telefonoTrabajo.validator}"/>
                                                    </af:inputText>
