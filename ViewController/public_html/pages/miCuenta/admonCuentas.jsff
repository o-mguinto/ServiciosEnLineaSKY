                                                                  required="#{bindings.UsuarioRegistro.hints.email.mandatory}"
                                                                  columns="#{bindings.UsuarioRegistro.hints.email.displayWidth}"
                                                                  maximumLength="#{bindings.UsuarioRegistro.hints.email.precision}"
                                                                  shortDesc="#{bindings.UsuarioRegistro.hints.email.tooltip}"
                                                                  id="it5" readOnly="true">
                                                        <f:validator binding="#{row.bindings.email.validator}"/>
                                                    </af:inputText>
